

document.addEventListener("DOMContentLoaded", function(event) {
	var el = document.getElementById('greet');
	var greetParamBtn = document.getElementById('greetparam');
	el.addEventListener("click", greet, false);
	greetParamBtn.addEventListener("click", function(){greetParam("billy")}, false);
});


function greet(){
    console.log('hello world');
}

function greetParam(x){
    console.log('hello' + x);
}
